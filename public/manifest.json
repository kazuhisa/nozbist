{
  "manifest_version": 3,
  "name": "nozbist",
  "description": "Nozbist is Nozbe chrome extension.",
  "version": "2.0",
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "identity"
  ],
  "host_permissions": [
    "https://api.nozbe.com:3000/*"
  ]
}